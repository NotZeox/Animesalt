{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "name": "anime-salt-api",
  "version": 2,
  "builds": [
    {
      "src": "src/server.js",
      "use": "@vercel/node@latest",
      "config": {
        "includeFiles": [
          "src/**/*.js",
          "src/**/*.json"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/src/server.js",
      "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS", "HEAD"]
    }
  ],
  "framework": null
}
